{
  "theme": {
    "@primary-color": "#dc6aac",
    "@link-color": "#dc6aac",
    "@border-radius-base": "2px",
    "@font-size-base": "16px",
    "@line-height-base": "1.2"
  },
  "proxy": {
    "/api": {
      "target": "http://localhost/"
    }
  },
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd-mobile", "style": "css" }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
