<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!-- 配置mysql 驱动jar包路径.用了绝对路径，要找到自己maven库中的驱动包位置 -->
	<classPathEntry
		location="E:\env\maven\.m2\repository\mysql\mysql-connector-java\5.1.38\mysql-connector-java-5.1.38.jar" />

	<context id="mysql_tables" targetRuntime="MyBatis3">

		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<!-- 数据库连接配置，一般不用变，大家用的都是一个库 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://103.234.126.8:3306/HCP?useSSL=false&amp;characterEncoding=utf-8"
			userId="lenovohit" password="lenovohit">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 数据表对应的model层，targetpackage对应的你模块要生成model的包名。而targetproject对应你的源码包名 -->
		<javaModelGenerator targetPackage="com.lenovohit.hcp.appointment.model"
			targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- sql mapper 隐射配置文件 -->
		<!-- <sqlMapGenerator targetPackage="com.yihaomen.mapper" targetProject="src"> 
			<property name="enableSubPackages" value="true" /> </sqlMapGenerator> -->

		<!-- 在ibatis2 中是dao层，但在mybatis3中，其实就是mapper接口 -->
		<!-- <javaClientGenerator type="XMLMAPPER" targetPackage="com.yihaomen.inter" 
			targetProject="src"> <property name="enableSubPackages" value="true" /> </javaClientGenerator> -->

		<!-- 要对那些数据表进行生成操作，必须要有一个. tablename对应你要生成的数据库表名字，domainobjectname对应你生成的model名字 -->
		<table schema="mybatis" tableName="reg_info" domainObjectName="RegInfo"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>

	</context>
</generatorConfiguration>